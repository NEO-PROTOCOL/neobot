{
  "name": "neo-igcli",
  "version": "1.0.0",
  "description": "The unofficial CLI and terminal client for Instagram",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "postbuild": "chmod +x dist/cli.js",
    "start": "node server.js & node dist/index.js",
    "dev": "rimraf dist && tsc -w",
    "build": "rimraf dist && tsc",
    "clean": "rm -rf dist",
    "start:cli": "node dist/cli.js",
    "start:mock": "node dist/mocks/cli.mock.js",
    "test": "xo && ava",
    "format": "prettier --write .",
    "lint-check": "xo"
  },
  "keywords": [
    "instagram",
    "cli",
    "terminal",
    "tui",
    "ink",
    "typescript",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@i7m/instagram-cli": "^1.4.0",
    "@inkjs/ui": "^2.0.0",
    "chalk": "^4.1.2",
    "chalk-animation": "^2.0.3",
    "cli-spinners": "^2.8.0",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "emoji-name-map": "^2.0.3",
    "emoji-regex": "^10.2.1",
    "file-type": "^21.0.0",
    "fs-extra": "^11.1.1",
    "fuse.js": "^7.1.0",
    "ink": "^5.0.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-picture": "^1.3.3",
    "ink-testing-library": "^4.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^8.2.5",
    "instagram_mqtt": "^1.2.3",
    "instagram-private-api": "^1.45.3",
    "jimp": "^0.22.8",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "meow": "^14.0.0",
    "mqtt": "^4.3.7",
    "node-fetch": "^2.6.11",
    "open": "^11.0.0",
    "pastel": "^4.0.0",
    "patch-package": "^8.0.1",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "read-package-up": "^12.0.0",
    "readline": "^1.3.0",
    "sharp": "^0.32.4",
    "terminal-image": "^2.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.21.0",
    "typescript": "^5.1.6",
    "unicode-emoji-json": "^0.8.0",
    "utf8": "^3.0.0",
    "util": "^0.12.5",
    "winston": "^3.10.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^8.1.0",
    "@types/chalk-animation": "^1.6.1",
    "@types/debug": "^4.1.12",
    "@types/inquirer": "^9.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.5",
    "@types/prompts": "^2.4.4",
    "@types/react": "^18.2.18",
    "@types/utf8": "^3.0.1",
    "ava": "^6.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "xo": "^0.57.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "xo --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prepare": "husky"
}
